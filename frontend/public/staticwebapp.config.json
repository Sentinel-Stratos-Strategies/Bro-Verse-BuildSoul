{
    "routes": [
        {
            "route": "/board",
            "rewrite": "/index.html"
        },
        {
            "route": "/*",
            "rewrite": "/index.html"
        }
    ],
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/*.css",
            "/*.js",
            "/*.map",
            "/*.json",
            "/assets/*",
            "/favicon.*",
            "/robots.txt",
            "/manifest.*"
        ]
    }
}